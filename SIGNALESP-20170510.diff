+	void setReceiveMode()
+	{
+		setIdleMode();
+		uint8_t maxloop = 0xff;
+
+		while (maxloop-- &&	(cmdStrobe(CC1100_SRX) & CC1100_STATUS_STATE_BM) != CC1100_STATE_RX) // RX enable
+			delay(1);
+		if (maxloop == 0 )		DBG_PRINTLN("CC1101: Setting RX failed");
+
+	}
+
+	void setTransmitMode()
