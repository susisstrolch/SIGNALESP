+bool SignalDetectorClass::decode(const int * pulse)
+{
+	success = false;
+
+	//int temp;
+	//*first = *last;
+	//*last = *pulse;
+	if (messageLen > 0)
+		last = &pattern[message[messageLen - 1]];
+	*first = *pulse;
+	
+	doDetect();
+	return success;
+}
