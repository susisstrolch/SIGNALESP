+		template <>
+		struct SimplifyMemFunc<SINGLE_MEMFUNCPTR_SIZE> {
+			template <class X, class XFuncType, class GenericMemFuncType>
+			inline static GenericClass *Convert(X *pthis, XFuncType function_to_bind,
